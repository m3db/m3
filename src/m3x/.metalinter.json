{
  "Linters": {
    "maptime": {
      "Command": "maptime",
      "Pattern": "PATH:LINE:COL:MESSAGE"
    }
  },
  "Enable":
    [ "deadcode"
    , "varcheck"
    , "structcheck"
    , "goconst"
    , "ineffassign"
    , "unconvert"
    , "misspell"
    , "unparam"
    , "megacheck"
    , "maptime" ],
  "Deadline": "3m",
  "EnableGC": true
}
