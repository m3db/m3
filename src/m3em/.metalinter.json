{
  "Linters": {
    "deadcode":  { "Command": "deadcode  -tags integration" },
    "varcheck":  { "Command": "varcheck  -tags integration" },
    "unparam":   { "Command": "unparam   -tags integration" },
    "megacheck": { "Command": "megacheck -tags integration" } },
  "Enable":
    [ "deadcode"
    , "varcheck"
    , "structcheck"
    , "goconst"
    , "ineffassign"
    , "unconvert"
    , "misspell"
    , "unparam"
    , "megacheck" ],
  "Deadline": "3m",
  "EnableGC": true
}
