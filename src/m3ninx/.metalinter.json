{
  "Linters": {
    "unused": {
      "Command": "unused -tags 'big'",
      "Pattern": "PATH:LINE:COL:MESSAGE"
    },
    "gosimple": {
      "Command": "gosimple -tags 'big'",
      "Pattern": "PATH:LINE:COL:MESSAGE"
    },
    "gofmt": {
      "Command": "gofmt",
      "Pattern": "PATH:LINE:COL:MESSAGE"
    }
  },
  "Enable": [
    "vet",
    "gofmt",
    "varcheck",
    "structcheck",
    "goconst",
    "ineffassign",
    "unconvert",
    "misspell",
    "golint",
    "gosimple",
    "unused",
    "maligned"
  ],
  "Deadline": "3m",
  "EnableGC": true
}