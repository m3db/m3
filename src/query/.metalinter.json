{
  "Linters": {
    "badtime": {
      "Command": "badtime",
      "Pattern": "PATH:LINE:COL:MESSAGE"
    },
    "importorder": {
      "Command": "importorder -patterns=\"STDLIB github.com/m3db EXTERNAL\"",
      "Pattern": "PATH:LINE:MESSAGE"
    }
  },
  "Enable":
    [ "deadcode"
    , "varcheck"
    , "structcheck"
    , "goconst"
    , "ineffassign"
    , "unconvert"
    , "misspell"
    , "unparam"
    , "megacheck"
    , "badtime"
    , "golint"
    , "importorder" ],
  "Deadline": "3m",
  "EnableGC": true
}
