{
  "Linters": {
    "unused": {
      "Command": "unused -tags 'integration big adhoc'",
      "Pattern": "PATH:LINE:COL:MESSAGE"
    },
    "gosimple": {
      "Command": "gosimple -tags 'integration big adhoc'",
      "Pattern": "PATH:LINE:COL:MESSAGE"
    },
    "badtime": {
      "Command": "badtime -tags 'integration big adhoc'",
      "Pattern": "PATH:LINE:COL:MESSAGE"
    }
  },
  "Enable":
    [ "varcheck"
    , "structcheck"
    , "goconst"
    , "ineffassign"
    , "unconvert"
    , "misspell"
    , "golint"
    , "gosimple"
    , "unused"
    , "badtime"
    , "maligned" ],
  "Deadline": "3m",
  "EnableGC": true }
